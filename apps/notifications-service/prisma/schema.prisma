generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Notification {
  id          String           @id @default(cuid())
  title       String
  body        String
  type        String
  sentAt      DateTime         @default(now())
  deliveredAt DateTime?
  userId      String
  swapId      String?

  @@map("notifications")
}
